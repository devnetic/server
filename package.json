{
  "name": "@devnetic/server",
  "version": "1.1.1",
  "description": "A modern, powerful and blasing fast server for Node.js",
  "main": "index.js",
  "scripts": {
    "coverage": "nyc ava",
    "lint": "standard",
    "prepublishOnly": "npm run lint && npm run test",
    "release": "standard-version",
    "start": "node index.js",
    "test": "ava --verbose",
    "test:watch": "ava --verbose --watch"
  },
  "ava": {
    "require": [
      "esm"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "files": [
    "src/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devnetic/server.git"
  },
  "keywords": [
    "server",
    "http",
    "api"
  ],
  "author": "Alvaro Jose Agamez Licha",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/devnetic/server/issues"
  },
  "homepage": "https://github.com/devnetic/server#readme",
  "dependencies": {
    "@devnetic/cli": "^1.2.1",
    "@devnetic/router": "^2.0.0",
    "esm": "^3.2.25"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@commitlint/format": "^11.0.0",
    "ava": "^3.13.0",
    "husky": "^4.3.0",
    "nyc": "^15.1.0",
    "sinon": "^9.2.1",
    "standard": "^16.0.1",
    "standard-version": "^9.0.0"
  }
}
